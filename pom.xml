<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.ilikly.finalframework</groupId>
    <artifactId>final-framework</artifactId>
    <!--<version>${final.release.version}</version>-->
    <version>0.0.1-SNAPSHOT</version>
    <modules>
        <module>final-core</module>
        <module>final-util</module>
        <module>final-data</module>
        <module>final-json</module>
        <module>final-cache</module>
        <module>final-mybatis</module>
        <module>final-coding</module>
        <module>final-spring</module>
        <module>final-test</module>
        <module>final-redis</module>
        <module>final-auth</module>
        <module>final-logging</module>
    </modules>
    <packaging>pom</packaging>

    <name>final-framework</name>
    <description>final framework for a simple restful api server</description>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.1.1.RELEASE</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <properties>
        <final.release.version>0.0.1-SNAPSHOT</final.release.version>
        <spring.boot.version>2.1.1.RELEASE</spring.boot.version>
        <!--<spring-cloud.version>Greenwich.RC2</spring-cloud.version>-->
        <spring-cloud.version>Finchley.RELEASE</spring-cloud.version>
        <mybatis.version>3.4.6</mybatis.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>1.18.0</version>
            <optional>true</optional>
            <scope>compile</scope>
        </dependency>
        <!--<dependency>-->
        <!--<groupId>org.springframework.boot</groupId>-->
        <!--<artifactId>spring-boot-starter-test</artifactId>-->
        <!--<scope>test</scope>-->
        <!--</dependency>-->
    </dependencies>

    <dependencyManagement>

        <dependencies>
            <!-- final-core -->
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!-- final-json -->
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-json</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-json-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-json-spring-boot-starter</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!-- final-redis -->
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-redis</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-redis-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-redis-autoconfigure</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-redis-spring-boot-starter</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!-- final-cache -->
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-cache</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-cache-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-cache-spring-boot-starter</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!-- final-data -->
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-data-annotation</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-data-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-data-autoconfigure</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-data-coding</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-data-spring-boot-starter</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!--final-mybatis-->
            <dependency>
                <groupId>org.mybatis</groupId>
                <artifactId>mybatis</artifactId>
                <version>${mybatis.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-mybatis-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-mybatis-agent</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-mybatis-autoconfigure</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-mybatis-coding</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-mybatis-spring-boot-starter</artifactId>
                <version>${project.version}</version>
            </dependency>

            <!--final-spring-->
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-spring</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-spring-coding</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-spring-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-spring-boot-starter</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-util</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-data</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-mybatis</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>com.ilikly.finalframework</groupId>
                <artifactId>final-coding</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-starter-data-redis</artifactId>
                <version>${spring.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.springframework.cloud</groupId>
                <artifactId>spring-cloud-dependencies</artifactId>
                <version>${spring-cloud.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <compilerArgument>-parameters</compilerArgument>
                    <source>8</source>
                    <target>8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

    <!--发布 -->
    <distributionManagement>
        <!--多点仓库-->
        <repository>
            <id>dmall.com</id>
            <url>http://nexus.dmall.com:8081/nexus/content/repositories/3rd</url>
        </repository>
        <snapshotRepository>
            <id>dmall.com</id>
            <url>http://nexus.dmall.com:8081/nexus/content/repositories/snapshots</url>
        </snapshotRepository>
    </distributionManagement>

    <repositories>
        <repository>
            <id>spring-milestones</id>
            <name>Spring Milestones</name>
            <url>https://repo.spring.io/milestone</url>
        </repository>
    </repositories>


</project>
