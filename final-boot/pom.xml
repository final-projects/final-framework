<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>final-frameworks</artifactId>
        <groupId>org.ifinal.finalframework.frameworks</groupId>
        <version>1.0.1-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.ifinal.finalframework.boot</groupId>
    <artifactId>final-boot</artifactId>
    <packaging>pom</packaging>
    <modules>
        <module>final-boot-autoconfigure</module>
        <module>final-boot-starter</module>
        <module>final-boot-starter-entity</module>
        <module>final-boot-starter-mybatis</module>
        <module>final-boot-starter-service</module>
        <module>final-boot-starter-json</module>
        <module>final-boot-starter-data</module>
        <module>final-boot-starter-redis</module>
        <module>final-boot-starter-cache</module>
        <module>final-boot-starter-monitor</module>
        <module>final-boot-starter-web</module>
        <module>final-boot-starter-zookeeper</module>
        <module>final-boot-starter-dashboard</module>
    </modules>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.ifinal.finalframework</groupId>
                <artifactId>final-framework</artifactId>
                <version>${final.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <!-- ==================================================================================================== -->
            <!-- =========================================== final-boot ============================================= -->
            <!-- ==================================================================================================== -->
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-autoconfigure</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-json</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-entity</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-mybatis</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-service</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-data</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-redis</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-cache</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-web</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-monitor</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-zookeeper</artifactId>
                <version>${final.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-dashboard</artifactId>
                <version>${final.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>


    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <configuration>
                    <attach>true</attach>
                </configuration>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>

                </executions>
            </plugin>
        </plugins>
    </build>

